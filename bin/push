#!/usr/bin/expect

#set timeout 30
set host [lindex $argv 0]

spawn ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -i /var/www/thub/bin/thub.pem "ubuntu@$host"

expect "$ "
send "cd /var/www/thub\r"

expect "# "
send "sudo bash ./bin/deploy.sh develop staging\r"
expect "Username for"
sleep 1
send "tk\r";
expect "Password for"
sleep 1
send "Riktam234\r";

interact
sleep 5
send "exit\r"
#interact
